{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "node --inspect-brk=127.0.0.1:9229 ../node_modules/.bin/vitest run --test-timeout=60000",
    "generate-data": "tsx ./data/generate.ts",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "tsx --inspect-brk=127.0.0.1:9230 src/index.ts",
    "build": "swc src -d ../dist/server",
    "build:tsc": "tsc -b",
    "clean": "rm -rf ../dist/server"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.21",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^3.1.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
